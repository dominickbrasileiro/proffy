{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "camelcase": "off",
      "class-methods-use-this": "off",
      "import/extensions": [
        "error",
        "ignorePackages",
        { "js": "never", "ts": "never" }
      ]
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
}
